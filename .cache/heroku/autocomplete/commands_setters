_set_domains_flags () {
_flags=(
"--help[(switch) Show CLI help.]"
"--app=-[(autocomplete) app to run command against]: :_compadd_flag_options"
"--remote=-[(autocomplete) git remote of app to use]: :_compadd_flag_options"
"--json[(switch) output in json format]"
"--columns=-[only show provided columns (comma-separated)]"
"--sort=-[property to sort by (prepend '-' for descending)]"
"--filter=-[filter property by partial string matching, ex: name=foo]"
"--csv[(switch) output is csv format [alias: --output=csv]]"
"--output=-[output in a more machine friendly format]"
"--extended[(switch) show extra columns]"
"--no-header[(switch) hide table header from output]"
)
}

_set_domains_add_flags () {
_flags=(
"--help[(switch) Show CLI help.]"
"--app=-[(autocomplete) app to run command against]: :_compadd_flag_options"
"--cert=-[the name of the SSL cert you want to use for this domain]"
"--json[(switch) output in json format]"
"--wait[(switch) undefined]"
"--remote=-[(autocomplete) git remote of app to use]: :_compadd_flag_options"
)
}

